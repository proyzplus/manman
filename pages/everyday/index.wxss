.msc-mask-layer { 
      width: 100%;
      height: 100%;
      position: fixed;
      top: 0;
      left: 0;
      /* background-color: rgba(0, 0, 0, 0.8); */
      background-repeat: no-repeat;
      background-size: cover;
      opacity: 1;
}

.userInfo {
      width: 92%;
      margin: auto;
      display: flex;
      justify-content: space-between;
}

.userAvatar {
      text-align: center;
}

.userAvatar image {
      width: 100rpx;
      height: 100rpx;
      border-radius: 50%;
      border: 1px solid #f3b9ab;
}


.bottomMiddleHeaderView {
      width: 256rpx;
      height: 256rpx;
      border-radius: 50%;
      background-color: none !important;
      font-size: 24rpx;
      margin: auto;
      margin-top: 320rpx;
}

.bottomMiddleHeaderView image {
      width: 230rpx;
      height: 230rpx;
}

button::after {
      border: none;
}

.datetime {
      width: 100%;
      margin-top: 12rpx;
      margin-bottom: 12rpx;
      text-align: center;
      color: #fff;
}

.times {
      text-align: center;
      letter-spacing: 4rpx;
      line-height: 64rpx;
}

.bottomImg {
      width: 240rpx;
      height: 80rpx;
      position: fixed;
      bottom: 48rpx;
      right: 0rpx;
}

.bottomImg image {
      width: 56rpx;
      height: 56rpx;
}

.bottomImg button {
      background: none;
      width: 100rpx;
      display: inline;
}