.type-content {
      position: absolute;
      left: 20rpx;
      right: 20rpx;
      text-align: center;
      color: #ffffff;
}

.content-name {
      position: relative;
      top: 108rpx;
      font-size: 52rpx;
      display: block;
}

.content-time {
      display: block;
      position: relative;
      top: 168rpx;
      font-size: 68rpx;
}