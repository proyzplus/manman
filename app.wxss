.type-card {
      width: 92%;
      height: 420rpx;
      margin: auto;
      border-radius: 8rpx;
      box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);
      padding: 16rpx;
      margin-top: 4px;
}

.type-back {
      background-size: 100% 100%;
      background-repeat: no-repeat;
      text-align: center;
      float: left;
      width: 100%;
      height: 420rpx;
      background-repeat: no-repeat;
      background-position: center;
      background-size: cover;
      /* -webkit-filter: blur(2px);
